{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nextbasic",
      "camelcase": "NextBASIC",
      "title": "NextBASIC",
      "scope": "source.nextbasic",
      "file-types": ["nxb", "bas.txt", "bas"],
      "injection-regex": "^nextbasic$",
      "class-name": "TreeSitterNextbasic"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "ZX Spectrum NextBASIC",
    "authors": [
      {
        "name": "Richard Taylor",
        "email": "moomerman@gmail.com",
        "url": "https://richardtaylor.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/moomerman/tree-sitter-nextbasic"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
