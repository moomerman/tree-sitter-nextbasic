{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "nextbasic",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}